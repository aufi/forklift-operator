---
apiVersion: forklift.konveyor.io/v1alpha1
kind: Provisioner
metadata:
  name: ocs-cephfs
  namespace: {{ app_namespace }}
spec:
  name: openshiftstorage.cephfs.csi.ceph.com
  features: []
  volumeModes:
  - name: Filesystem
    priority: 0
    features: []
    accessModes:
    - name: ReadWriteMany
      priority: 0
      features:
      - LiveMigration
    - name: ReadWriteOnce
      priority: 0
      features: []
